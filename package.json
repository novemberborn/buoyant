{
  "name": "buoyant",
  "version": "0.2.0",
  "description": "Framework for implementing servers using the Raft Consensus Algorithm",
  "keywords": [
    "raft",
    "consensus"
  ],
  "author": "Mark Wubben (http://novemberborn.net/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/novemberborn/buoyant"
  },
  "homepage": "https://github.com/novemberborn/buoyant",
  "bugs": "https://github.com/novemberborn/buoyant/issues",
  "license": "ISC",
  "engines": {
    "node": ">=5.0.0"
  },
  "main": "main.js",
  "files": [
    "main.js",
    "main.js.map",
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf main.js main.js.map lib",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir ./ --source-maps",
    "prepublish": "npm run build",
    "example": "node bin/example.js",
    "lint": "standard",
    "test": "mocha",
    "posttest": "npm run lint",
    "precoverage": "rimraf coverage",
    "coverage": "nyc --reporter=lcov --reporter=text npm test"
  },
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-core": "^6.3.15",
    "babel-eslint": "^4.1.6",
    "babel-plugin-espower": "^2.0.0",
    "babel-plugin-transform-async-to-generator": "^6.3.13",
    "babel-plugin-transform-es2015-destructuring": "^6.3.15",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.3.16",
    "babel-plugin-transform-es2015-parameters": "^6.3.13",
    "babel-plugin-transform-strict-mode": "^6.3.13",
    "chalk": "^1.1.1",
    "lolex": "^1.3.2",
    "mocha": "^2.3.4",
    "nyc": "^5.0.0",
    "power-assert": "^1.2.0",
    "proxyquire": "^1.7.3",
    "rimraf": "^2.4.4",
    "sinon": "^2.0.0-pre",
    "source-map-support": "^0.4.0",
    "standard": "^5.4.1"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
