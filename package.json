{
  "name": "buoyant",
  "version": "0.2.0",
  "description": "Framework for implementing servers using the Raft Consensus Algorithm",
  "keywords": [
    "raft",
    "consensus"
  ],
  "author": "Mark Wubben (http://novemberborn.net/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/novemberborn/buoyant"
  },
  "homepage": "https://github.com/novemberborn/buoyant",
  "bugs": "https://github.com/novemberborn/buoyant/issues",
  "license": "ISC",
  "engines": {
    "node": ">=5.0.0"
  },
  "main": "main.js",
  "files": [
    "main.js",
    "main.js.map",
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf main.js main.js.map lib",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir ./ --source-maps",
    "prepublish": "npm run build",
    "example": "node bin/example.js",
    "lint": "standard",
    "test": "mocha",
    "posttest": "npm run lint",
    "coverage": "bin/coverage.sh"
  },
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.5",
    "babel-plugin-espower": "^2.0.0",
    "babel-plugin-transform-async-to-generator": "^6.1.18",
    "babel-plugin-transform-es2015-destructuring": "^6.1.18",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.1.20",
    "babel-plugin-transform-es2015-parameters": "^6.1.18",
    "babel-plugin-transform-strict-mode": "^6.1.18",
    "chalk": "^1.1.1",
    "istanbul": "^0.4.0",
    "lolex": "^1.3.2",
    "mocha": "^2.3.4",
    "nyc": "^3.2.2",
    "power-assert": "^1.2.0",
    "proxyquire": "^1.7.3",
    "remap-istanbul": "^0.4.0",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.2",
    "source-map-support": "^0.3.3",
    "standard": "^5.4.0"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
